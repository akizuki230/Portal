<%= simple_form_for(@articulo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :titulo %>
    <%= f.input :contenido,:class => 'ckeditor' %>
    <%= f.input :user_id %>
    <%= f.file_field :imagen %>
    
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
