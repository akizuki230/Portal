<%= simple_form_for(@integrante) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombre %>
    <%= f.input :apellido_1 %>
    <%= f.input :apellido_2 %>
    <%= f.input :perfil %>
    <%= f.input :telefono %>
    <%= f.input :imagen %>
    <%= f.input :user_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
