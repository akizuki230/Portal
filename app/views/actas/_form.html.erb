<%= simple_form_for(@acta) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :fecha %>
    <%= f.input :lugar %>
    <%= f.input :tipo_reunion %>
    <%= f.input :orden_dia %>
    <%= f.input :desarrollo %>
    <%= f.input :concluciones %>
    <%= f.input :tareas %>
    <%= f.input :user_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
